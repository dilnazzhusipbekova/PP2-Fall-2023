d = int(input())
if((c == a +1 or c==a-1) and (d == b+2 or d == b-2)):
    print("YES")
elif((c==a+2 or c==a-2) and (d==b+1 or d==b-1)):
    print("YES")
else:
    print("NO")

#task 12
m = int(input())
n = int(input())
k = int(input())
if(k<(n*m) and ((k % n == 0) or (k % m == 0))):
    print("YES")
else:
    print("NO")

#task 13
n = int(input())
m = int(input())
x = int(input())
y = int(input())
if(m < n):
    n, m = m ,n
xmin = min(x, n-x)
ymin = min(y, m-y)
print(min(xmin, ymin))

